{
  "name": "@unquote/game-generator",
  "version": "0.3.1",
  "private": true,
  "files": [
    "dist",
    "!dist/testing.*"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing.d.ts",
      "import": "./dist/testing.js"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint .",
    "format": "oxfmt ."
  },
  "dependencies": {
    "@opentelemetry/api": "catalog:prod",
    "luxon": "catalog:prod",
    "typebox": "catalog:prod"
  },
  "devDependencies": {
    "@opentelemetry/context-async-hooks": "catalog:testing",
    "@opentelemetry/sdk-trace-base": "catalog:testing",
    "@tsconfig/node24": "catalog:build",
    "@tsconfig/strictest": "catalog:build",
    "@types/luxon": "catalog:build",
    "@types/node": "catalog:build",
    "fast-check": "catalog:testing",
    "nanoid": "catalog:testing",
    "oxfmt": "catalog:lint",
    "oxlint": "catalog:lint",
    "rimraf": "catalog:build",
    "typescript": "catalog:build",
    "vitest": "catalog:testing"
  }
}
