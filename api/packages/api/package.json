{
  "name": "@unquote/api",
  "version": "0.1.0",
  "private": true,
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "dev": "tsx watch --import ./src/instrumentation.ts src/index.ts",
    "start": "node --import ./dist/instrumentation.js dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@eropple/fastify-openapi3": "catalog:prod",
    "@fastify/cors": "catalog:prod",
    "@fastify/env": "catalog:prod",
    "@fastify/helmet": "catalog:prod",
    "@fastify/rate-limit": "catalog:prod",
    "@fastify/sensible": "catalog:prod",
    "@fastify/type-provider-typebox": "catalog:prod",
    "@fastify/under-pressure": "catalog:prod",
    "@opentelemetry/api": "catalog:prod",
    "@opentelemetry/exporter-trace-otlp-http": "catalog:prod",
    "@opentelemetry/instrumentation": "catalog:prod",
    "@opentelemetry/instrumentation-fastify": "catalog:prod",
    "@opentelemetry/instrumentation-http": "catalog:prod",
    "@opentelemetry/instrumentation-pino": "catalog:prod",
    "@opentelemetry/instrumentation-undici": "catalog:prod",
    "@opentelemetry/resources": "catalog:prod",
    "@opentelemetry/sdk-node": "catalog:prod",
    "@opentelemetry/semantic-conventions": "catalog:prod",
    "@unquote/game-generator": "workspace:*",
    "ajv-formats": "catalog:prod",
    "awilix": "catalog:prod",
    "fastify": "catalog:prod",
    "fastify-plugin": "catalog:prod",
    "luxon": "catalog:prod",
    "pino": "catalog:prod",
    "sqids": "catalog:prod",
    "typebox": "catalog:prod"
  },
  "devDependencies": {
    "@tsconfig/node24": "catalog:build",
    "@tsconfig/strictest": "catalog:build",
    "@types/luxon": "catalog:build",
    "@types/node": "catalog:build",
    "@vitest/coverage-v8": "catalog:testing",
    "ajv": "catalog:testing",
    "fast-check": "catalog:testing",
    "rimraf": "catalog:build",
    "tsx": "catalog:build",
    "typescript": "catalog:build",
    "vitest": "catalog:testing"
  }
}
